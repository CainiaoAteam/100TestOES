<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    	<class name="com.oes.bean.TestPaper" table="testpaper">
		<id name="tpid" column="tpid"  type="integer">
			<generator class="native"/>
		</id>
		
		<property name="tpno" column="tpno"  type="string"/>
		<property name="tpname" column="tpname"  type="string"/>
		<property name="squestionscore" column="squestionscore"  type="integer"/>
		<property name="mquestionscore" column="mquestionscore"  type="integer"/>
		<property name="fquestionscore" column="fquestionscore"  type="integer"/>
		<property name="totalscore" column="totalscore"  type="integer"/>
		<property name="state" column="state"  type="integer"/>
		
		<!--list 还没配置完成啊 -->
		
		<list name="squestions" table="singlequestions">  
            
            <!-- 映射集合属性 表的外键列 -->  
            <key column="tpid" not-null="false"></key> 
            
            <!-- 映射集合属性表的集合索引列 -->  
            <list-index column="sqid"></list-index>  
            <element type="string" column=""/> 
            
        </list>  
		

		<!-- 配置的多方 name是JavaBean属性名称  class="一方类的全路径" cloumn="外键的名称"  -->
		<many-to-one name="" class="" column=""/>
		
	</class>
    </hibernate-mapping>