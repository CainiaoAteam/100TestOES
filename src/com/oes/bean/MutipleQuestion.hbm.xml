<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    	<class name="com.oes.bean.MutipleQuestion" table="multipquestion">
		<id name="mqid" column="mqid"  type="integer">
			<generator class="native"/>
		</id>
		
		<property name="mqno" column="mqno"  type="string"/>
		<property name="mquestion" column="mquestion"  type="string"/>
		<property name="mchoiceA" column="mchoiceA"  type="string"/>
		<property name="mchoiceB" column="mchoiceB"  type="string"/>
		<property name="mchoiceC" column="mchoiceC"  type="string"/>
		<property name="mchoiceD" column="mchoiceD"  type="string"/>
		<property name="manswer" column="manswer"  type="string"/>
		<property name="mexplanation" column="mexplanation"  type="string"/>
		<property name="difficulty" column="difficulty"  type="string"/>
		

		<!-- 配置的多方 name是JavaBean属性名称  class="一方类的全路径" cloumn="外键的名称"  -->
		<many-to-one name="" class="" column=""/>
		
	</class>
    </hibernate-mapping>